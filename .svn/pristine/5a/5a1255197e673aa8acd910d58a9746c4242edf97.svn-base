.share1 {DISPLAY: block; FLOAT: left; WIDTH: 380px}
.share1 span {float:left;padding-left:1px;}

.share {
	DISPLAY: block; FLOAT: left; WIDTH: 380px
}
.share UL {
	PADDING-RIGHT: 0px; DISPLAY: block; PADDING-LEFT: 0px; PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-TOP: 0px; LIST-STYLE-TYPE: none; HEIGHT: 24px
}
.share UL LI {
	BORDER-RIGHT: #cadbe7 0px solid; BORDER-TOP: #cadbe7 0px solid; DISPLAY: block; FLOAT: left; BORDER-LEFT: #cadbe7 0px solid; MARGIN-RIGHT: 3px; BORDER-BOTTOM: #cadbe7 0px solid; HEIGHT: 22px
}
.share UL LI A {
	PADDING-RIGHT: 3px; DISPLAY: block; PADDING-LEFT: 23px;PADDING-BOTTOM: 3px; CURSOR: pointer; LINE-HEIGHT: 16px; PADDING-TOP: 3px; HEIGHT: 16px
}
.share UL LI A:hover {
	BACKGROUND-COLOR: #ecf8fc
}
.share UL LI A.addfav {
	BACKGROUND-POSITION: 3px 0px; WIDTH: 62px
}
.share UL LI A.transmit {
	BACKGROUND-POSITION: 3px -44px; WIDTH: 62px
}
.share UL LI A.toshare {
	BACKGROUND-POSITION: 3px -66px; WIDTH: 62px
}
.share UL LI A.renren {
	BACKGROUND-POSITION: 3px -88px
}
.share UL LI A.kaixin {
	BACKGROUND-POSITION: 3px -154px
}
.share UL LI A.douban {
	BACKGROUND-POSITION: 3px -176px
}
.share UL LI A.guge {
	BACKGROUND-POSITION: 3px -198px
}
.share UL LI A.qq {
	BACKGROUND-POSITION: 3px -220px
}
.share UL LI A.baidu {
	BACKGROUND-POSITION: 3px -242px
}
.share UL LI A.delicious {
	BACKGROUND-POSITION: 3px -110px
}
.share UL LI A.bai {
	BACKGROUND-POSITION: 3px -132px
}
.share UL LI A.sinat {
	BACKGROUND-POSITION: 3px -264px
}
.share UL LI A.live {
	BACKGROUND-POSITION: 3px -286px
}
.share UL LI A.digu {
	BACKGROUND-POSITION: 3px -308px
}
.share UL LI A.yahoo {
	BACKGROUND-POSITION: 3px -330px
}
.share UL LI A.rline {
	MARGIN-RIGHT: 8px
}
.share UL LI .webs SPAN A {
	DISPLAY: block; OVERFLOW: hidden; WIDTH: 55px
}
.webs {
	CLEAR: both; BORDER-RIGHT: #cadbe7 1px solid; PADDING-RIGHT: 3px; BORDER-TOP: #cadbe7 1px solid; DISPLAY: none; PADDING-LEFT: 5px;BACKGROUND: url(../Images/share.gif) #fff no-repeat 88px -347px; PADDING-BOTTOM: 3px; MARGIN-LEFT: -1px; OVERFLOW: hidden; BORDER-LEFT: #cadbe7 1px solid; WIDTH: 170px; PADDING-TOP: 3px; BORDER-BOTTOM: #cadbe7 1px solid; POSITION: absolute
}
.webs SPAN {
	DISPLAY: block; FLOAT: left; OVERFLOW: hidden; FONT-FAMILY: arial; HEIGHT: 22px
}
.shownav .webs {
	DISPLAY: block
}